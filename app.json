{
  "expo": {
    "name": "riyadah",
    "slug": "riyadah",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "splash": {
      "backgroundColor": "#111111"
    },
    "scheme": "riyadah",
    "deepLinking": true,
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "bundleIdentifier": "app.riyadah",
      "associatedDomains": [
        "applinks:riyadah.app"
      ],
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow access to your camera to take profile pictures",
        "NSPhotoLibraryUsageDescription": "Allow access to your photos to choose a profile picture"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.omarsaab96.riyadah",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "riyadah.app",
              "pathPrefix": "/posts"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "react-native-compressor"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6ad7cb3c-f873-489a-8120-c3e2da4bf98f"
      }
    }
  }
}