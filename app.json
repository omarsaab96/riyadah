{
  "expo": {
    "name": "Riyadah",
    "slug": "riyadah",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "scheme": "riyadah",
    "deepLinking": true,
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "bundleIdentifier": "app.omarsaab96.riyadah",
      "associatedDomains": [
        "applinks:riyadah.app",
        "applinks:riyadah.app"
      ],
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow access to your camera to take profile pictures",
        "NSPhotoLibraryUsageDescription": "Allow access to your photos to choose a profile picture",
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDQ5HceUQFK_NhNAcKg6kCncREr7t60sP8"
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBJelab1iC0Cf-AS9Lenz2RzGVmdQra1hc"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#111111"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.omarsaab96.riyadah",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "riyadah.app",
              "pathPrefix": "/posts"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "react-native-compressor"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6ad7cb3c-f873-489a-8120-c3e2da4bf98f"
      },
      "REVENUECAT_IOS": "appl_BKrhCsqwkBqSmIgcnyeAlYorukq",
      "REVENUECAT_ANDROID": "goog_qiQzoKrtCxfqFKQwHDYltBiIUKV"
    }
  }
}